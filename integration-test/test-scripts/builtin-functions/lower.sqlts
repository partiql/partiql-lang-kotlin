// null propagation -----------------------------------------------------------------------------------------------
for::{
    template: [
        test::{
            name: "lower null and missing propagation",
            sql: "lower(${param})",
            expected: $result
        }
    ],
    variableSets: [
        { param: "null",    result: result::null },
        { param: "missing", result: result::null },
    ]
}

// wrong number of arguments --------------------------------------------------------------------------------------
for::{
    template: [
        test::{
            name: "lower wrong number of arguments",
            sql: "${sql}",
            expected: error::{
                code: EVALUATOR_INCORRECT_NUMBER_OF_ARGUMENTS_TO_FUNC_CALL,
                properties: {
                    LINE_NUMBER:1,
                    COLUMN_NUMBER:1,
                    EXPECTED_ARITY_MIN:1,
                    EXPECTED_ARITY_MAX:1
                }
            }
        }
    ],
    variableSets: [
        { sql: "lower()" },
        { sql: "lower('', '')" },
        { sql: "lower('', 1)" },
    ]
}

// wrong type of arguments ----------------------------------------------------------------------------------------
for::{
    template: [
        test::{
            name: "lower wrong type of arguments",
            sql: "lower(${param})",
            expected: error::{
                code: "null",
                properties: {
                    LINE_NUMBER:1,
                    COLUMN_NUMBER:1,
                }
            }
        }
    ],
    variableSets: [
        { param: "1" },
        { param: "1.0" },
        { param: "1e0" },
        { param: "`()`" },
        { param: "true" },
        { param: "`[]`" },
        { param: "`{}`" },
    ]
}

